.TOSNotification-left {
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  -o-box-flex: 1;
  box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
}
.TOSNotification-right {
  margin-top: 1rem;
}
@media only screen and (min-width: 48em) {
  .TOSNotification-right {
    margin: 0 0 0 1rem;
  }
}
.TOSNotification-right .btn {
  width: 100%;
  text-align: center;
}
.TOSNotification-cross {
  position: absolute;
  right: 0.5rem;
  top: 0.75rem;
  cursor: pointer;
}
.TOSNotification-cross .icon-cross {
  color: #ff4842;
}
@media only screen and (min-width: 48em) {
  .TOSNotification-cross {
    top: 0.5rem;
    right: 0.5rem;
  }
