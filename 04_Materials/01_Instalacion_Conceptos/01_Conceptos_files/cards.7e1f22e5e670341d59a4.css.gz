/* #u-left
Flota un elemento a la izquierda
*/
/*
  <div class="">
    <div class="u-left u-border">izquierda</div>
  </div>

*/
.u-left {
  float: left;
}
/* #u-right
Flota un elemento a la derecha
*/
/*
  <div class="">
    <div class="u-right u-border">derecha</div>
  </div>

*/
